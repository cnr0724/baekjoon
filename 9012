//https://www.acmicpc.net/problem/9012

#include<iostream>
#include<string>
#include<stack>
#include<cstring>

using namespace std;

bool check(char* c) {
	stack<char> stack;

	for (int i = 0; i < strlen(c); i++) {
		if (c[i] == '(') {
			stack.push(c[i]);
		}
		else {
			if (stack.empty()) {
				return false;
			}
			else {
				stack.pop();
			}
		}
	}

	return stack.empty();
}

int main(int argc, char** argv)
{
	int n;
	cin >> n;
	for (int t = 0; t < n; t++) {
		char c[50];
		cin >> c;
		if (check(c)) {
			cout << "YES"<<endl;
		}
		else {
			cout << "NO"<<endl;
		}
	}
	return 0;
}
