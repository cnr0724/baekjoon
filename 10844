#include <iostream>
#include <algorithm>
using namespace std;

int main()
{
	int num;
	cin >> num;
	int count[101][10];
	for (int i = 0; i < 10; i++) {
		if (i == 0)		count[1][i] = 0;
		else			count[1][i] = 1;
	}
	for (int i = 2; i <= num; i++) {
		for (int j = 0; j < 10; j++) {
			if (j == 0) count[i][j] = count[i - 1][1];
			else if (j == 9) count[i][j] = count[i - 1][8];
			else
			{
				count[i][j] = (int)(count[i - 1][j - 1] + count[i - 1][j + 1])%1000000000;
			}
		}
	}
	int result = 0;
	for (int j = 0; j < 10; j++) {
		result += count[num][j];
		result %= 1000000000;
	}
	cout << result << endl;
	return 0;
}
