//틀렸던 코드
#include <iostream>
using namespace std;

int main()
{
	int a, b, test_case;
	cin >> test_case;
	for (int i = 0; i < test_case; i++) {
		cin >> a;
		cin >> b;
		cout << "Case #" << (i + 1) << ": " <<a<<' + '<<b<<' = '<< (a + b)<<endl;
	}
	return 0;
}

//맞았던 코드
#include <iostream>
using namespace std;

int main()
{
	int a, b, test_case;
	cin >> test_case;
	for (int i = 0; i < test_case; i++) {
		cin >> a;
		cin >> b;
		cout << "Case #" << (i + 1) << ": " <<a<<" + "<<b<<" = "<< (a + b)<<endl;
	}
	return 0;
}
